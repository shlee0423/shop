<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <settings>
        <setting name="jdbcTypeForNull" value="NULL"/>
        <setting name="cacheEnabled" value="true"/>
        <setting name="mapUnderscoreToCamelCase" value="true"/>
        <setting name="defaultFetchSize" value="100"/>
        <setting name="defaultStatementTimeout" value="25"/>
    </settings>

    <typeAliases>
        <typeAlias type="com.korea.dto.FileDTO" alias="FileDTO" />

        <typeAlias type="com.korea.dto.user.UserDTO" alias="UserDto" />
        <typeAlias type="com.korea.dto.user.SocialUserDTO" alias="SocialUserDTO" />

        <typeAlias type="com.korea.dto.ProductDTO" alias="ProductDTO" />

        <typeAlias type="com.korea.dto.customer_service.OneOnOneDTO" alias="OneOnOneDTO" />
        <typeAlias type="com.korea.dto.customer_service.OneOnOneAnswerDTO" alias="OneOnOneAnswerDTO" />
    </typeAliases>

</configuration>